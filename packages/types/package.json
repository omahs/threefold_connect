{
  "name": "types",
  "version": "1.0.0",
  "description": "shared types",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "rm -rf dist/* && tsc -p tsconfig.json && yarn build:babel",
    "build:babel": "babel index.ts -d dist --extensions \".ts,.tsx\" --source-maps inline"
  },
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/plugin-transform-typescript": "^7.18.4",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "class-validator": "^0.13.2",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.18.2"
  }
}
